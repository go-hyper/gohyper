{
  "manifest_version": 2,
  "name": "Test",
  "description": "This is a test version.",
  "version": "1.0",

  "icons": {
    "16": "images/test.png",
    "32": "images/test.png",
    "48": "images/test.png",
    "64": "images/test.png",
    "128": "images/test.png"
  },

  "permissions": ["tabs", "http://*/*", "https://*/*", "storage", "https://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular.min.js"],

  "applications": {
    "gecko": {
      "id": "test@test.de"
    }
  },

  "background": {
    "scripts": ["js/background.js", "js/gohyper.js"],
    "persistent": false
  },

  "options_ui": {
    "page": "popup.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_icon": {
      "19": "images/test.png",
      "38": "images/test.png"
    },
    "default_title": "Test!",
    "default_popup": "popup.html"
  }
}
